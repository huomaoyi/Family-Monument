.page {
  width: 720rpx;
  height: 1000rpx;
  background-color: #f5f5f5;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  margin: auto;
}

.title {
  margin: auto;
  width: 500rpx;
  height: 100rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  text-align: center;
  margin-top: 5rpx;
  margin-bottom: 8rpx;
}

.cover-container {
  width: 700rpx;
  height: 800rpx;
  background-color: #f5f5f5; 
  margin: auto;
}

.cover-title {
  width: 300rpx;
  text-align: center;
  margin: auto;
  margin-top: 100rpx;
}

.cover-content {
  width: 600rpx;
  margin: auto;
  margin-top: 20rpx;
}

.container {
  width: 700rpx;
  height: 1000rpx;
  margin: auto;
  

  display: flex;
  flex-direction: column;
  justify-content: space-between;
  /* border-width: 2rpx;
  border-style: dotted; */
}

.userinfo-button{
  width: 100%;
  height: 100%;
  margin: auto;
  z-index: 100;
  position: absolute;
}

.user-info-btn {
  width: 100% !important;
  height: 100%;
}

.name {
  width: 600rpx;
  height: 50rpx;
  margin-top: 200rpx;
  margin-bottom: 30rpx;
  padding-left: 20rpx;
  font-size: 40rpx;
  text-align: center;
  color: white;
}

.live {
  width: 400rpx;
  height: 30rpx;
  padding-left: 200rpx;
  margin-top: 15rpx;
  font-size: 28rpx;
  text-align: left;
  color: white;

  display: flex;
  flex-direction: row;
  justify-content: center;
}

.time-text {
  width: 70rpx;
}

.word {
  margin: auto;
  margin-top: 60rpx;
  width: 360rpx;
  height: 300rpx;
}

.word-text {
  margin-left: -10rpx;
  width: 100%;
  height: 290rpx;
  font-size: 28rpx;
  color: white;
  /* font-family: SimHei; */

  overflow:hidden; 
  text-overflow:ellipsis;
  /* white-space:nowrap; */
}

.action-container {
  height: 150rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;

  /* border-style: dotted;
  border-width: 2rpx;  */
}

.action {
  font-size: 25rpx;
  width: 150rpx;
  height: 56rpx;
  line-height: 25rpx;
  padding-top: 6rpx;
  margin-right: 5rpx;
  margin-top: 10rpx;
  color: #ffffff;
  border-radius: 10rpx;
  background-color: #4c5b6f;
}

.action view{
  text-align: center;
  /* margin-top: -5rpx; */
}

.send-action {
  font-size: 25rpx;
  width: 280rpx;
  height: 56rpx;
  margin: auto;
  line-height: 56rpx;
  text-align: center;
  margin-top: 10rpx;
  color: #ffffff;
  border-radius: 10rpx;
  background-color: #4c5b6f;
}

.operator-container {
  width: 650rpx;
  height: 60rpx;
  display: flex;
  margin-top: 20rpx;
  flex-direction: row;
  justify-content: center;

  /* border-style: dotted;
  border-width: 2rpx;
  border-color: red; */
}

.operator-container button {
  min-height: 30rpx;
  height: 60rpx;
  /* margin: 10rpx 5rpx; */
  text-align: center;
  font-size: 28rpx;
  line-height: 40rpx;
  color: white;
}

.logo{
  width: 100%;
  position: fixed;
  bottom:0px;
  display: flex;
  justify-content: center;
}
 
.copyright-view{
  margin-top: -20rpx;
  position: fixed;
  text-align: center;
  width: 100%;
  height: 55rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  bottom:0rpx;
  color: #acabab;
  background-color: #f5f5f5;
  z-index: -1;
}

.card-container {
  width:650rpx;
  height:1000rpx;
  margin: auto;
  background-color: #ffffff;
  border-radius: 10rpx;
  border: 1px solid rgba(0, 0, 0, 0.1);
  position: absolute;
  
  background-size: 100% 100%;
  background-image: url('data:image/jpeg;base64,')
}